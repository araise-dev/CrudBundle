{
    "name": "whatwedo/crud-bundle",
    "type": "symfony-bundle",
    "description": "Very simple CRUD operations",
    "license": "MIT",
    "homepage": "https://doc.whatwedo.ch/whatwedo/crudbundle",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "whatwedo GmbH",
            "email": "welove@whatwedo.ch"
        }
    ],
    "require": {
        "php":                      ">=8.0",
        "symfony/framework-bundle": "^5.3",
        "symfony/http-kernel":      "^5.3",
        "symfony/security-bundle":  "^5.3",
        "symfony/serializer":       "^5.3",
        "symfony/form":             "^5.3",
        "symfony/twig-bundle":      "^5.3",
        "whatwedo/core-bundle":     "^1.0",
        "whatwedo/table-bundle":    "^1.0",
        "phpspec/prophecy": "~1.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2"
    },
    "suggest": {
        "mhujer/breadcrumbs-bundle": "Allows creation of breadcrumbs",
        "knplabs/knp-menu-bundle": "Allows creation of breadcrumbs"
    },
    "autoload": {
        "psr-4": {
            "whatwedo\\CrudBundle\\": ""
        }
    }
}
